@model IEnumerable<LampStatus>

@{
    ViewData["Title"] = "All Statuses";
}

<div class="text-center">
    <h1 class="display-4">All Statuses</h1>
</div>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>Status from</th>
            <th>Lamp ID</th>
            <th>Lamp Level</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var lampStatus in Model)
        {
            <tr>
                <td>@lampStatus.CreateTime</td>
                <td>@lampStatus.LampId</td>
                <td>@lampStatus.LampLevel</td>
            </tr>
        }
    </tbody>
</table>